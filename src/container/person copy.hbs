<div>
    {{#with person}}
        <main>
            {{> title big=true data='General Information'}}
            <hr />

            <section>
                <div>
                    <strong>
                        Person
                    </strong>

                    <hr />
                    <br />
                    
                    <b>Name:</b>
                    {{name}}
                    <br />

                    <b>Age:</b>
                    {{age}}
                    <br />

                    <b>Gender:</b>
                    {{gender}}
                    <br />
                </div>
            </section>

            <section>
                {{#with address}}
                    <div>
                        <strong>
                            Address
                        </strong>

                        <hr />
                        <br />

                        <b>Street:</b>
                        {{street}}
                        <br/>

                        <b>City:</b>
                        {{city}}
                        <br />

                        <b>State:</b>
                        {{state}}
                        <br />

                        <b>Zip:</b>
                        {{zip}}
                        <br />

                        <b>Country</b>
                        {{country}}
                        <br />
                    </div>
                {{/with}}
            </section>

            <section>
                {{#each education}}
                    <div>   
                        <strong>
                            Education
                        </strong>

                        <hr />
                        <br />

                        <ul>
                            <li>
                                <div>
                                    <b>Degree:</b>
                                    {{degree}}
                                    <br />

                                    <b>Major:</b>
                                    {{major}}
                                    <br />

                                    <b>University:</b>
                                    {{university}}
                                    <br />

                                    <b>Graduation Year:</b>
                                    {{graduationYear}}
                                    <br/>
                                </div>
                            </li>
                        </ul>
                    </div>
                {{/each}}
            </section>

            <section>
                {{#each workExperience}}
                    <strong>
                        Work Expirience
                    </strong>

                    <hr />
                    <br />

                    <ul>
                        <li>
                            <div>
                                <b>Company:</b>
                                {{company}}
                                <br />

                                <b>Title:</b>
                                {{title}}
                                <br />

                                <b>Start:</b>
                                <time datetime="{{startDate}}">
                                    {{startDate}}
                                </time>
                                <br />

                                <b>End:</b>
                                <time datetime="{{endDate}}">
                                    {{endDate}}
                                </time>
                                <br />

                                {{#each responsibilities}}
                                    <p>
                                        {{this}}
                                    </p>
                                {{/each}}

                                <b>Year Founded:</b>
                                {{year_founded}}
                                <br />

                                <b>Industry:</b>
                                {{industry}}
                                <br />
                                <br />

                                <b>Employees</b>

                                <hr />
                                <br />

                                {{#each employees}}
                                    <b>Name:</b>
                                    {{name}}
                                    <br />

                                    <b>Position:</b>
                                    {{position}}
                                    <br />

                                    <b>Department:</b>
                                    {{department}}
                                    <br />
                                    <br>

                                    <b>Projects</b>
                                    <br />
                                    <br />

                                    <ul>
                                        <li>
                                            <div>
                                                {{#each projects}}
                                                    {{name}}
                                                    <br />

                                                    <p>{{description}}</p>
                                                    <br />
                                                    
                                                    <b>Status:</b>
                                                    {{status}}
                                                    <br />
                                                    <br />

                                                    <b>Teams</b>
                                                    <br />
                                                    <br />

                                                    <ul>
                                                        <li>
                                                            <div>
                                                                {{#each teams}}
                                                                    {{team_name}}

                                                                    <br />

                                                                    {{#with team_leader}}
                                                                        <b>{{title}}</b>
                                                                        <br />

                                                                        <b>Name:</b>
                                                                        {{name}}
                                                                        <br />

                                                                        <b>Email:</b>
                                                                        {{email}}
                                                                        <br />
                                                                    {{/with}}
                                                                    
                                                                    <br />
                                                                    <br />

                                                                    {{#each team_members}}
                                                                        <b>Name:</b>
                                                                        {{name}}
                                                                        <br />

                                                                        <b>Title:</b>
                                                                        {{title}}
                                                                        <br />

                                                                        <b>Email:</b>
                                                                        {{email}}
                                                                        <br />

                                                                        <b>Skills:</b>
                                                                        {{#each skills}}
                                                                            <ul>
                                                                                <li>
                                                                                    <div>
                                                                                        {{this}}
                                                                                    </div>
                                                                                </li>
                                                                            </ul>
                                                                        {{/each}}

                                                                        <br />
                                                                        <br />

                                                                        <b>Projects</b>
                                                                        <br />

                                                                        {{#each projects}}
                                                                            <ul>
                                                                                <li>
                                                                                    <div>
                                                                                        {{name}}
                                                                                        <br />

                                                                                        <p>{{description}}</p>
                                                                                        <br />

                                                                                        <b>Technologies:</b>
                                                                                        <br />

                                                                                        {{#each technologies}}
                                                                                            <ul>
                                                                                                <li>
                                                                                                    <div>
                                                                                                        {{this}}
                                                                                                    </div>
                                                                                                </li>
                                                                                            </ul>
                                                                                        {{/each}}
                                                                                        
                                                                                        <br />
                                                                                        <br />
                                                                                        
                                                                                        <b>Members:</b>
                                                                                        <br />

                                                                                        {{#each team_members}}
                                                                                            <ul>
                                                                                                <li>
                                                                                                    <div>
                                                                                                        <b>Name:</b>
                                                                                                        {{name}}
                                                                                                        <br />

                                                                                                        <b>Title:</b>
                                                                                                        {{title}}
                                                                                                        <br />
                                                                                                    </div>
                                                                                                </li>
                                                                                            </ul>
                                                                                        {{/each}}
                                                                                    </div>
                                                                                </li>
                                                                            </ul>
                                                                        {{/each}}
                                                                    {{/each}}
                                                                {{/each}}
                                                            </div>
                                                        </li>
                                                    </ul>
                                                {{/each}}
                                            </div>
                                        </li>
                                    </ul>
                                {{/each}}
                            </div>
                        </li>
                    </ul>
                {{/each}}
            </section>
        </main>
    {{/with}}
</div>