<main>
    <section>
        {{#with listPages}}
            <div class='mt-5 mb-3'>
                <h3>{{title}}</h3>
            </div>
            <div class='mb-5'>
                <p>{{description}}</p>
            </div>
            <div>
                {{#each buttons}}
                    <div class='mb-2'>
                        <a href='{{href}}' class='btn btn-primary col-2'>{{text}}</a>
                    </div>
                {{/each}}
            </div>
        {{/with}}
    </section>
</main>